php:
   memory_limit: "512M"
   max_input_time: 120
   max_execution_time: 600
   expose_php: "Off"
   file_uploads: "On"
   upload_max_filesize: "20M"
   max_file_uploads: "20"
   date_timezone: "America/Los_Angeles"
   date_default_latitude: "46.7311399"
   date_default_longitude: "-117.1603188"
   error_reporting: "E_ALL & ~E_DEPRECATED & ~E_STRICT"
   post_max_size: "50M"
